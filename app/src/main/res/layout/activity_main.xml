<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView4"
        style="@style/icon"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="68dp"
        app:srcCompat="@drawable/personal" />

    <ImageView
        android:id="@+id/imageView5"
        style="@style/icon"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView4"
        android:layout_marginEnd="38dp"
        app:srcCompat="@drawable/publicicon" />

    <ImageView
        android:id="@+id/imageView6"
        style="@style/icon"
        android:layout_alignTop="@+id/imageView4"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/supervise" />

    <TextView
        android:id="@+id/textView4"
        style="@style/textBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="15dp"
        android:text="Ferol LabExam1" />

    <TextView
        android:id="@+id/textView"
        style="@style/textBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="178dp"
        android:text="Personal Notes" />

    <TextView
        android:id="@+id/textView2"
        style="@style/textBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView"
        android:layout_centerHorizontal="true"
        android:text="Office Notes" />

    <TextView
        android:id="@+id/textView3"
        style="@style/textBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView5"
        android:layout_alignTop="@+id/textView"
        android:text="Public Notes" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="226dp"
        android:background="@color/colorAccent"
        android:visibility="visible" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="44dp"
        android:background="@color/colorAccent"
        android:visibility="visible" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="324dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="192dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="182dp"
                android:layout_height="115dp"
                android:backgroundTint="@color/note1" >

                <TextView
                    android:id="@+id/textView9"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="20dp"
                    android:text="To do List" />

                <TextView
                    android:id="@+id/textView11"
                    style="@style/textBox1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="70dp"
                    android:text="Request ToR" />

                <TextView
                    android:id="@+id/textView12"
                    style="@style/textBox1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="50dp"
                    android:text="Accomplish Chapter 1" />

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="182dp"
                android:layout_height="115dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="@color/note2" >

                <TextView
                    android:id="@+id/textView13"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="20dp"
                    android:text="Movies to Watch:" />

                <TextView
                    android:id="@+id/textView14"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="60dp"
                    android:text="The Hows of Us" />

                <TextView
                    android:id="@+id/textView15"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="40dp"
                    android:text="Goyo: Ang Batang Heneral " />
            </android.support.v7.widget.CardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    app:srcCompat="@drawable/marvel" />
            </LinearLayout>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/note3">

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="81dp"
                    android:layout_marginTop="5dp"
                    app:srcCompat="@drawable/goyo" />

                <TextView
                    android:id="@+id/textView16"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="100dp"
                    android:text="Goyo: Ang Batang Heneral Main Characters" />

                <TextView
                    android:id="@+id/textView18"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="150dp"
                    android:text="Paulo Avelino - Goyo" />

                <TextView
                    android:id="@+id/textView19"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="170dp"
                    android:text="Carlos Aquino - Col. Enriquez " />

                <TextView
                    android:id="@+id/textView20"
                    style="@style/textBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="190dp"
                    android:text="Mon Confiado- Pres. Emilio A." />
            </android.support.v7.widget.CardView>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>